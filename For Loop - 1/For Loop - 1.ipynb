   "source": [
    "You are given a number n, you need to print its multiplication table in a single line."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Value of n = 4\n",
      "4 * 1 = 4\n",
      "4 * 2 = 8\n",
      "4 * 3 = 12\n",
      "4 * 4 = 16\n",
      "4 * 5 = 20\n",
      "4 * 6 = 24\n",
      "4 * 7 = 28\n",
      "4 * 8 = 32\n",
      "4 * 9 = 36\n",
      "4 * 10 = 40\n"
     ]
    }
   ],
   "source": [
    "def multiplication():\n",
    "    n = int(input(\"Enter a number: \"))\n",
    "\n",
    "    print(\"Value of n =\", n)\n",
    "\n",
    "    for i in range(1, 11):\n",
    "        print(n, \"*\", i, \"=\", n * i)\n",
    "\n",
    "multiplication()"
   ]
